<!--housedetails.wxml-->
<swiper class="house_swiper" circular="true" indicator-active-color="#00ce3c" indicator-color="" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for-items="{{imgUrls}}" wx:key="*this">
    <swiper-item>
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>
<view class="house_name" id="{{eachId}}">
  <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell">
          <view class="weui-cell__bd">象屿两江御府</view>
          <view class="weui-cell__ft">期房</view>
      </view>
  </view>
  <view class="house_content">
    <view class="weui-media-box__desc">别墅：<text class="money">310万</text><text class="money_unit">元/套</text></view>
    <block wx:for="{{my_availability}}" wx:key="*this">
      <view class="weui-media-box__desc">
        <view class="introduction" wx:for="{{item.introduce}}" wx:for-item="introduce" wx:key="introduce">
          {{introduce.name}}
        </view>
      </view>
    </block>
    <view class="weui-media-box__desc">地址：<text class="right_content">北碚-照母山</text></view>
    <block wx:for="{{house_content}}" wx:key="*this">
      <view class="weui-media-box__desc">
        {{item.name}}：
        <text class="right_content">{{item.content}}</text>
      </view>
    </block>
  </view>
</view>
<view class="huxing">
  <view class="weui-cells weui-cells_after-title">
      <navigator url="../allhousestyle/allhousestyle" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__bd">主力户型（5）</view>
          <view class="weui-cell__ft weui-cell__ft_in-access"></view>
      </navigator>
  </view>
  <scroll-view scroll-x style="width:100%;" class="huxing_box">
    <view class="huxing_all_box">
      <block wx:for="{{huxing_content}}" wx:key="*this">
        <navigator url="../hxdetails/hxdetails" class="huxing_list" hover-class="weui-cell_active">
          <view class="list_img">
            <image src="{{item.imageUrl}}"></image>
          </view>
          <view class="list_title">{{item.title}}</view>
          <view class="list_style">{{item.style}}</view>
        </navigator>
      </block>
    </view>
  </scroll-view>
</view>
<view class="consultant">
  <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell">
          <view class="weui-cell__bd">置业顾问</view>
      </view>
  </view>
  <view class="weui-panel weui-panel_access">
    <block wx:for="{{consultant}}" wx:key="*this">
      <view class="weui-panel__bd">
          <view class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
              <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
                  <image class="weui-media-box__thumb" src="{{item.imgUrl}}" />
              </view>
              <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
                  <view class="weui-media-box__title">{{item.name}}</view>
                  <view class="weui-media-box__desc">
                    联系电话：{{item.phone}}
                    <image type="default" bindtap="callPhone" class="phone_img" src="../../images/电话.png"></image>
                  </view>
              </view>
          </view>
      </view>
    </block>
  </view>
</view>
